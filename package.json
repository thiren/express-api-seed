{
  "name": "express-api-seed",
  "version": "1.0.0",
  "description": "A project seed for new node APIs built using Express",
  "main": "./bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "watch": "nodemon ./bin/www",
    "lint": "eslint ./",
    "test": "NODE_ENV=test mocha",
    "coverage": "NODE_ENV=test nyc node_modules/mocha/bin/_mocha",
    "test-windows": "set NODE_ENV=test&& mocha",
    "coverage-windows": "set NODE_ENV=test&& nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiren/express-api-seed.git"
  },
  "keywords": [
    "api",
    "rest",
    "node",
    "express"
  ],
  "author": "Thiren Bunsee",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/thiren/express-api-seed/issues"
  },
  "homepage": "https://github.com/thiren/express-api-seed#readme",
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "async": "3.1.0",
    "body-parser": "1.19.0",
    "boom": "7.3.0",
    "config": "3.2.2",
    "cors": "2.8.5",
    "express": "4.17.1",
    "helmet": "3.20.0",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "mongodb": "3.2.7",
    "morgan": "1.9.1",
    "uuid": "3.3.2",
    "winston": "3.2.1",
    "winston-loggly-bulk": "3.0.1"
  },
  "optionalDependencies": {
    "bson-ext": "2.0.2"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2"
  }
}
