{
  "name": "node-api-seed",
  "version": "0.0.0",
  "description": "A project seed for new node APIs",
  "main": "./bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "watch": "nodemon ./bin/www",
    "lint": "eslint ./",
    "test": "set NODE_ENV=test&& mocha",
    "coverage": "set NODE_ENV=test&& istanbul cover node_modules/mocha/bin/_mocha -- -R spec",
    "test-linux": "NODE_ENV=test mocha",
    "coverage-linux": "NODE_ENV=test istanbul cover node_modules/mocha/bin/_mocha -- -R spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiren/node-api-seed.git"
  },
  "keywords": [
    "node",
    "express",
    "mocha",
    "chai",
    "rest",
    "restful"
  ],
  "author": "Thiren Bunsee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thiren/node-api-seed/issues"
  },
  "homepage": "https://github.com/thiren/node-api-seed#readme",
  "engines": {
    "node": "6.9.1"
  },
  "dependencies": {
    "app-root-path": "2.0.1",
    "async": "2.1.2",
    "body-parser": "1.15.2",
    "boom": "4.2.0",
    "config": "1.24.0",
    "cors": "2.8.1",
    "cron": "1.1.1",
    "debug": "2.3.2",
    "express": "4.14.0",
    "lodash": "4.17.2",
    "moment": "2.16.0",
    "morgan": "1.7.0",
    "uuid": "3.0.0",
    "winston": "2.3.0",
    "winston-daily-rotate-file": "1.3.1",
    "winston-loggly": "1.3.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "3.10.2",
    "istanbul": "0.4.5",
    "mocha": "3.1.2",
    "nodemon": "1.11.0",
    "supertest": "2.0.1"
  }
}
