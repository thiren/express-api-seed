{
  "name": "node-api-seed",
  "version": "0.0.0",
  "description": "A project seed for new node APIs",
  "main": "./bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "watch": "nodemon ./bin/www",
    "lint": "eslint ./",
    "test": "set NODE_ENV=test&& mocha",
    "coverage": "set NODE_ENV=test&& istanbul cover node_modules/mocha/bin/_mocha -- -R spec",
    "test-linux": "NODE_ENV=test mocha",
    "coverage-linux": "NODE_ENV=test istanbul cover node_modules/mocha/bin/_mocha -- -R spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiren/node-api-seed.git"
  },
  "keywords": [
    "node",
    "express",
    "api"
  ],
  "author": "Thiren Bunsee",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/thiren/node-api-seed/issues"
  },
  "homepage": "https://github.com/thiren/node-api-seed#readme",
  "engines": {
    "node": "~6.11"
  },
  "dependencies": {
    "app-root-path": "2.0.1",
    "async": "2.5.0",
    "body-parser": "1.17.2",
    "boom": "5.1.0",
    "config": "1.26.1",
    "cors": "2.8.3",
    "cron": "1.2.1",
    "express": "4.15.3",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "morgan": "1.8.2",
    "uuid": "3.1.0",
    "winston": "2.3.1",
    "winston-daily-rotate-file": "1.4.6",
    "winston-loggly-bulk": "2.0.0"
  },
  "devDependencies": {
    "chai": "4.0.2",
    "eslint": "4.1.1",
    "istanbul": "0.4.5",
    "mocha": "3.4.2",
    "nodemon": "1.11.0",
    "supertest": "3.0.0"
  }
}
